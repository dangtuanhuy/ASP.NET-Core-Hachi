@model Hachi.Models.SubCategory

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<br />
<h2>Create Sub Category</h2>
<br />

<hr />

<form asp-action="Delete">
    <div class="form-border">
        <input type="hidden" asp-for="Id" />
        <fieldset disabled>
            <div class="form-group row">
                <div class="col-2">
                    <label asp-for="SubCategoryName" class="control-label"></label>
                </div>
                <div class="col-4">
                    <input asp-for="SubCategoryName" class="form-control" />
                </div>

            </div>
            <div class="form-group row">
                <div class="col-2">
                    <label asp-for="Category.CategoryName" class="control-label"></label>
                </div>
                <div class="col-4">

                    <input asp-for="Category.CategoryName" class="form-control" />
                </div>

            </div>
        </fieldset>
        <br />
        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-primary" />&nbsp;
            <a asp-action="Index" class="btn btn-success">Index</a>
        </div>
    </div>
</form>

